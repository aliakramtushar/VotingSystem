@model IEnumerable<DataLayer.Employee>

@{
    ViewBag.Title = "ShowFinalResult";
    Layout = "~/Views/Shared/_UserLayout.cshtml";
}

<h2>ShowFinalResult</h2>


<table border="1">
    <tr>
        <th>
            @Html.DisplayName("Candidate Name")
        </th>
        <th>
            @Html.DisplayName("Candidate Id")
        </th>
       
       <th>
            @Html.DisplayName("Department Name")
        </th>
        <th>
            @Html.DisplayName("Total Vote")
        </th>
        
        
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Employee_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Employee_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Department_id )
        </td>
       
        
        <td>
            @Html.DisplayFor(modelItem => item.Count)
        </td>
        
       
    </tr>
}

</table>
<br />
<form method="post">
    <input id="Submit1" type="submit" value="Publish Result" />
</form>

